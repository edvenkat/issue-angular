{
  "name": "events-dynamic-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve -c=dev",
    "stage": "ng serve -c=stage",
    "production": "ng serve -c=production",
    "build": "ng build && node copy-amp",
    "lint": "eslint -f html -o ./reports/eslint-report.html --color ./",
    "e2e": "ng e2e",
    "build:browser:serverless": "ng build --prod --base-href /production/ && node copy-amp",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless && node copy-amp",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy && node copy-amp",
    "deploy": "serverless deploy",
    "build:server:serverless": "ng run events-dynamic-form:server:serverless && webpack --config webpack.server.config.js --progress --color",
    "compile:server": "webpack --config webpack.server.config.js --progress --color",
    "build:client-and-server-bundles": "ng build --prod && ng run events-dynamic-form:server:production --bundleDependencies all",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js",
    "build:server:dev": "ng run events-dynamic-form:server:dev && webpack --config webpack.server.config.js --progress --color",
    "build:browser:dev": "ng build --configuration production --c dev --base-href /",
    "build:dev": "cross-env NODE_ENV=dev node set-env.ts && npm run build:browser:dev && node copy-amp",
    "build:stage": "cross-env NODE_ENV=stg node set-env.ts && npm run build:browser:stage && npm run build:server:stage && node copy-amp",
    "build:server:stage": "ng run events-dynamic-form:server:stage && webpack --config webpack.server.config.js --progress --color",
    "build:browser:stage": "ng build --configuration production --c stage --base-href /",
    "build:uat": "cross-env NODE_ENV=stg node set-env.ts && npm run build:browser:uat && npm run build:server:uat && node copy-amp",
    "build:server:uat": "ng run events-dynamic-form:server:uat && webpack --config webpack.server.config.js --progress --color",
    "build:browser:uat": "ng build --configuration production --c uat --base-href /",
    "build:server:prod": "ng run events-dynamic-form:server:production && webpack --config webpack.server.config.js --progress --color",
    "build:browser:prod": "ng build --configuration production --c production",
    "build:prod": "cross-env NODE_ENV=prod node set-env.ts && npm run build:browser:prod && npm run build:server:prod && node copy-amp",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "serve:ssr:events-dynamic-form": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/platform-server": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@angular/ssr": "^17.1.1",
    "@contentful/rich-text-html-renderer": "^14.1.2",
    "@contentful/rich-text-react-renderer": "^14.1.3",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@ng-toolkit/serverless": "^8.0.3",
    "@ng-toolkit/universal": "^8.0.3",
    "aws-sdk": "^2.601.0",
    "aws-serverless-express": "^3.4.0",
    "contentful": "^8.5.8",
    "core-js": "^3.25.5",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "cross-env": "7.0.2",
    "domino": "^2.1.4",
    "ejs": "^3.1.8",
    "express": "^4.19.2",
    "fs": "0.0.1-security",
    "fs-extra": "^9.1.0",
    "hammerjs": "^2.0.8",
    "ng-lazyload-image": "^9.1.3",
    "ngx-cookie": "^4.1.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "rxjs": "^7.5.7",
    "serverless-api-compression": "^1.0.1",
    "sha256": "^0.2.0",
    "ts-loader": "^4.2.0",
    "tslib": "^2.0.0",
    "typescript": "5.3.3",
    "webpack": "^5.90.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@angular/language-service": "^17.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/node": "^16.11.43",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^12.2.0",
    "localstorage-polyfill": "^1.0.1",
    "opencollective": "^1.0.3",
    "protractor": "~7.0.0",
    "serverless": "^3.31.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-associate-waf": "^1.2.1",
    "serverless-offline": "^5.12.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^9.1.1",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "verbose": true,
    "modulePaths": [
      "<rootDir>"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}